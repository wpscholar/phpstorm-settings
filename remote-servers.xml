<application>
  <component name="RemoteServers">
    <remote-server name="Docker (Local by Flywheel)" type="docker">
      <configuration>
        <option name="apiUrl" value="https://192.168.95.100:2376" />
        <option name="certificatesPath" value="$USER_HOME$/.docker/machine/machines/local-by-flywheel" />
        <option name="machine" value="local-by-flywheel" />
        <option name="pathMappings">
          <list>
            <DockerPathMappingImpl>
              <option name="localPath" value="/Users" />
              <option name="virtualMachinePath" value="/Users" />
            </DockerPathMappingImpl>
            <DockerPathMappingImpl>
              <option name="localPath" value="/tmp" />
              <option name="virtualMachinePath" value="/tmp" />
            </DockerPathMappingImpl>
            <DockerPathMappingImpl>
              <option name="localPath" value="/Volumes" />
              <option name="virtualMachinePath" value="/Volumes" />
            </DockerPathMappingImpl>
            <DockerPathMappingImpl>
              <option name="localPath" value="/private" />
              <option name="virtualMachinePath" value="/private" />
            </DockerPathMappingImpl>
          </list>
        </option>
      </configuration>
    </remote-server>
  </component>
</application>